{
  "author": {
    "email": "support@xyo.network",
    "name": "XYO Development Team",
    "url": "https://xyo.network"
  },
  "bugs": {
    "email": "support@xyo.network",
    "url": "https://github.com/XYOracleNetwork/sdk-xyo-client-js/issues"
  },
  "workspaces": [
    "packages/**/*"
  ],
  "dependencies": {
    "@xyo-network/account": "workspace:^",
    "@xyo-network/api": "workspace:^",
    "@xyo-network/archivist": "workspace:^",
    "@xyo-network/boundwitness": "workspace:^",
    "@xyo-network/core": "workspace:^",
    "@xyo-network/cryptomarket-witness": "workspace:^",
    "@xyo-network/domain": "workspace:^",
    "@xyo-network/ethereum-witness": "workspace:^",
    "@xyo-network/network": "workspace:^",
    "@xyo-network/panel": "workspace:^",
    "@xyo-network/payload": "workspace:^",
    "@xyo-network/payload-plugin": "workspace:^",
    "@xyo-network/schema-witness": "workspace:^",
    "@xyo-network/storage": "workspace:^",
    "@xyo-network/systeminfo-witness": "workspace:^",
    "@xyo-network/typeof": "workspace:^",
    "@xyo-network/utils": "workspace:^",
    "@xyo-network/wallet": "workspace:^",
    "@xyo-network/witness": "workspace:^",
    "@xyo-network/witnesses": "workspace:^"
  },
  "description": "Primary SDK for using XYO Protocol 2.0",
  "devDependencies": {
    "@babel/core": "^7.18.10",
    "@babel/preset-env": "^7.18.10",
    "@xylabs/eslint-config": "^2.5.13",
    "@xylabs/ts-scripts": "^2.5.13",
    "@xylabs/tsconfig": "^2.5.13",
    "eslint": "^8.21.0",
    "jest": "^28.1.3",
    "jest-environment-jsdom": "^28.1.3",
    "jest-extended": "^3.0.1",
    "license-checker": "^25.0.1",
    "rimraf": "^3.0.2",
    "typedoc": "^0.23.10",
    "typescript": "^4.7.4"
  },
  "browser": "dist/esm/index.js",
  "docs": "dist/docs.json",
  "exports": {
    ".": {
      "node": {
        "import": "./dist/esm/index.js",
        "require": "./dist/cjs/index.js"
      },
      "browser": {
        "import": "./dist/esm/index.js",
        "require": "./dist/cjs/index.js"
      },
      "default": "./dist/esm/index.js"
    },
    "./dist/docs.json": {
      "default": "./dist/docs.json"
    },
    "./package.json": "./package.json"
  },
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "homepage": "https://xyo.network",
  "license": "LGPL-3.0-only",
  "name": "@xyo-network/sdk-xyo-client-js",
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/XYOracleNetwork/sdk-xyo-client-js.git"
  },
  "typedoc": {
    "entryPoint": "./src/index.ts",
    "readmeFile": "./README.md",
    "displayName": "Root Package"
  },
  "scripts": {
    "build": "echo Building... && yarn compile && yarn gen:docs",
    "compile": "echo Compiling... && yarn compile:esm && yarn compile:cjs && yarn lint-fast",
    "compile:cjs": "echo Compiling CJS... && yarn workspaces foreach -ptA run compile:package:cjs",
    "compile:esm": "echo Compiling ESM... && yarn workspaces foreach -ptA run compile:package:esm",
    "gen:docs": "echo Gen Docs... && yarn workspaces foreach -pA --exclude \"@xyo-network/sdk-xyo-client-js\" run gen:docs:package",
    "lint-pkg": "npmPkgJsonLint .",
    "deploy3": "yarn deploy-patch",
    "deploy-major": "yarn workspaces foreach --all version major --deferred && yarn deploy-common",
    "deploy-minor": "yarn workspaces foreach --all version minor --deferred && yarn deploy-common",
    "deploy-patch": "yarn workspaces foreach --all version patch --deferred && yarn deploy-common",
    "deploy-common": "echo Deploy... && yarn clean && yarn build && yarn apply-version:private && yarn publish:private",
    "apply-version:private": "echo Applying Version... && yarn version apply --all",
    "publish:private": "echo Publishing... && yarn workspaces foreach -pt npm publish",
    "clean": "echo Cleaning... && yarn workspaces foreach -pA run clean:package",
    "deps": "yarn workspaces foreach -pAv run deps:package",
    "lint": "echo Linting... && yarn eslint .",
    "lint-fast": "echo Linting fast... && yarn eslint . --cache",
    "lint-profile": "echo Profiling Lint... && TIMING=1 && yarn eslint .",
    "cycle": "yarn workspaces foreach -pA run cycle:package",
    "fix": "yarn workspaces foreach -pA run fix:package",
    "outdated": "yarn workspaces foreach -pA outdated",
    "up": "yarn workspaces foreach -pA up",
    "updo": "yarn workspaces foreach -pA run updo3:package",
    "gen:docs:package": "typedoc --logLevel Error --tsconfig $INIT_CWD/tsconfig.build.cjs.json --excludeExternals $INIT_CWD/src/index.ts --json $INIT_CWD/dist/docs.json",
    "clean:package": "yarn rimraf -q $INIT_CWD/dist && yarn rimraf -q $INIT_CWD/build",
    "fix:package": "yarn eslint $INIT_CWD/src --fix",
    "cycle:package": "yarn eslint --rule \"import/no-cycle:[1, { maxDepth: 10 }]\" $INIT_CWD/src",
    "deps:package": "depcheck $INIT_CWD",
    "updo3:package": "yarn update-interactive",
    "compile:package:cjs": "tsc -p $INIT_CWD/tsconfig.build.cjs.json",
    "compile:package:esm": "tsc -p $INIT_CWD/tsconfig.build.esm.json",
    "test": "yarn jest .",
    "license": "yarn license-checker --exclude \"MIT, ISC, Apache-2.0, BSD, BSD-2-Clause, CC-BY-4.0, Unlicense, CC-BY-3.0, CC0-1.0\""
  },
  "sideEffects": false,
  "types": "dist/esm/index.d.ts",
  "version": "2.24.4",
  "packageManager": "yarn@3.2.2"
}
