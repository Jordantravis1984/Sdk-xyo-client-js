{
  "name": "@xyo-network/sdk-xyo-mongo-js",
  "version": "2.43.31",
  "homepage": "https://xyo.network",
  "bugs": {
    "url": "https://github.com/XYOracleNetwork/sdk-xyo-mongo-js/issues",
    "email": "support@xyo.network"
  },
  "description": "Base functionality used throughout XYO TypeScript/JavaScript libraries that access Mongo DB",
  "author": {
    "name": "XYO Development Team",
    "url": "https://xyo.network",
    "email": "support@xyo.network"
  },
  "license": "LGPL-3.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/XYOracleNetwork/sdk-xyo-mongo-js.git"
  },
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/types/index.d.ts",
  "browser": {
    "mongodb": false
  },
  "exports": {
    ".": {
      "node": {
        "import": "./dist/esm/index.js",
        "require": "./dist/cjs/index.js"
      },
      "default": "./dist/esm/index.js"
    },
    "./package.json": "./package.json"
  },
  "keywords": [
    "xyo",
    "utility",
    "typescript",
    "mongo"
  ],
  "sideEffects": false,
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@xylabs/assert": "^2.7.1",
    "@xylabs/delay": "^2.7.1",
    "@xylabs/forget": "^2.7.1",
    "async-mutex": "^0.4.0",
    "mongodb": "^5.0.1"
  },
  "devDependencies": {
    "@xylabs/ts-scripts-yarn3": "^2.14.1",
    "@xylabs/tsconfig": "^2.14.1",
    "typescript": "^4.9.5"
  },
  "packageManager": "yarn@3.1.1"
}
