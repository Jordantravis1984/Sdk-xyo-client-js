  "name": "@xyo-network/node-core-query-handlers",
  "publishConfig": {
    "access": "public"
  },
  "version": "2.43.11",
  "description": "XYO Archivist",
  "sideeffects": false,
  "engines": {
    "node": ">=16 <17"
  },
  "esm5": "dist/esm/index.js",
