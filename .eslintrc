{
  "root": true,
  "extends": [
    "@xylabs"
  ],
  "parserOptions": {
    "project": "tsconfig.json"
  },
  "ignorePatterns": [
    "**/dist",
    "**/node_modules"
  ],
  "rules": {
    "import/no-internal-modules": [
      "warn", {
        "allow": [
          "lodash/*"
        ]
      }
    ]
  }
}
