{
  "author": {
    "email": "support@xyo.network",
    "name": "XYO Development Team",
    "url": "https://xyo.network"
  },
  "bugs": {
    "email": "support@xyo.network",
    "url": "https://github.com/XYOracleNetwork/sdk-xyo-client-js/issues"
  },
  "workspaces": [
    "packages/**/*"
  ],
  "dependencies": {
    "@xyo-network/modules": "workspace:^",
    "@xyo-network/plugins": "workspace:^",
    "@xyo-network/sdk": "workspace:^"
  },
  "description": "Primary SDK for using XYO Protocol 2.0",
  "devDependencies": {
    "@babel/core": "^7.20.12",
    "@babel/preset-env": "^7.20.2",
    "@types/node": "^18.11.18",
    "@xylabs/eslint-config": "^2.12.30",
    "@xylabs/ts-scripts-yarn3": "^2.12.30",
    "@xylabs/tsconfig": "^2.12.30",
    "babel-jest": "^29.4.1",
    "dotenv": "^16.0.3",
    "eslint": "^8.33.0",
    "jest": "^29.4.1",
    "jest-environment-jsdom": "^29.4.1",
    "jest-extended": "^3.2.3",
    "jest-sorted": "^1.0.14",
    "nodemon": "^2.0.20",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "typedoc": "^0.23.24",
    "typescript": "^4.9.5"
  },
  "resolutions": {
    "bn.js": "^5"
  },
  "browser": "dist/esm/index.js",
  "docs": "dist/docs.json",
  "exports": {
    ".": {
      "node": {
        "import": "./dist/esm/index.js",
        "require": "./dist/cjs/index.js"
      },
      "browser": {
        "import": "./dist/esm/index.js",
        "require": "./dist/cjs/index.js"
      },
      "default": "./dist/esm/index.js"
    },
    "./dist/docs.json": {
      "default": "./dist/docs.json"
    },
    "./package.json": "./package.json"
  },
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "homepage": "https://xyo.network",
  "license": "LGPL-3.0-only",
  "name": "@xyo-network/sdk-xyo-client-js",
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/XYOracleNetwork/sdk-xyo-client-js.git"
  },
  "typedoc": {
    "entryPoint": "./src/index.ts",
    "readmeFile": "./README.md",
    "displayName": "Root Package"
  },
  "scripts": {
    "coverage": "yarn jest --coverage --forceExit",
    "lint-pkg": "npmPkgJsonLint .",
    "start": "nodemon",
    "test": "jest --no-cache --forceExit",
    "test:ci": "jest --runInBand --coverage --forceExit --passWithNoTests",
    "xyo": "node ./packages/cli/dist/cjs/index.js",
    "xyo-ts": "yarn workspace @xyo-network/node-cli xyo-ts"
  },
  "sideEffects": false,
  "types": "dist/types/index.d.ts",
  "version": "2.43.24",
  "packageManager": "yarn@3.4.1",
  "volta": {
    "node": "18.13.0",
    "yarn": "1.22.19"
  }
}
